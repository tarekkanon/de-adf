{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafact90134"
		},
		"AdventureWorksDB_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AdventureWorksDB_service'"
		},
		"evpd_parquet_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'evpd_parquet_service'"
		},
		"evpdcsv_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'evpdcsv_service'"
		},
		"staging_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'staging_service'"
		},
		"synapse_dw_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'synapse_dw_service'"
		},
		"evpd_parquet_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tkstorageetltest.dfs.core.windows.net/"
		},
		"evpdcsv_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tkstorageetltest.dfs.core.windows.net/"
		},
		"staging_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tkstorageetltest.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafact90134_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdventureWorksDB_service_connectionString": {
						"value": "[parameters('AdventureWorksDB_service_connectionString')]"
					},
					"evpd_parquet_service_accountKey": {
						"value": "[parameters('evpd_parquet_service_accountKey')]"
					},
					"evpdcsv_service_accountKey": {
						"value": "[parameters('evpdcsv_service_accountKey')]"
					},
					"staging_service_accountKey": {
						"value": "[parameters('staging_service_accountKey')]"
					},
					"synapse_dw_service_connectionString": {
						"value": "[parameters('synapse_dw_service_connectionString')]"
					},
					"evpd_parquet_service_properties_typeProperties_url": {
						"value": "[parameters('evpd_parquet_service_properties_typeProperties_url')]"
					},
					"evpdcsv_service_properties_typeProperties_url": {
						"value": "[parameters('evpdcsv_service_properties_typeProperties_url')]"
					},
					"staging_service_properties_typeProperties_url": {
						"value": "[parameters('staging_service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "datafact90134_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafact90134_ArmTemplate_0')]"
			]
		}
	]
}